{
    "name": "nalrep/laravel",
    "description": "AI-powered, schema-aware, dynamic report generation for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Emmanuel Arhinful",
            "email": "kofibusy@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "ai",
        "reporting",
        "nlp",
        "database",
        "eloquent",
        "openai",
        "llm",
        "natural language",
        "sql",
        "query builder"
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "openai-php/client": "^0.8.0",
        "dompdf/dompdf": "^3.1",
        "symfony/http-client": "^7.3",
        "nyholm/psr7": "^1.8"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Nalrep\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nalrep\\NalrepServiceProvider"
            ],
            "aliases": {
                "Nalrep": "Nalrep\\Facades\\Nalrep"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}